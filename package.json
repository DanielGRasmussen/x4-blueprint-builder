	"private": true,
	"type": "module",
	"dependencies": {
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"web-vitals": "^5.0.3"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^6.6.4",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/jest": "^30.0.0",
		"@types/node": "^24.1.0",
